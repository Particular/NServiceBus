<?xml version="1.0"?>
<configuration>

  <configSections>
    <section name="UnicastBusConfig" type="NServiceBus.Config.UnicastBusConfig, NServiceBus.Core"/>   
  </configSections>

  <UnicastBusConfig>
    <MessageEndpointMappings>
      <add Messages="MyMessages" Endpoint="orderserviceinputqueue10"/>
    </MessageEndpointMappings>
  </UnicastBusConfig>
  
  <system.web>
    <authentication mode="Windows"/>
    <compilation debug="true"/>
  </system.web>
</configuration>